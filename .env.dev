# Crawl4AI Development Environment Variables

# LLM Provider API Keys (mocked for development)
OPENAI_API_KEY=sk-mock-openai-key-for-development
DEEPSEEK_API_KEY=mock-deepseek-api-key-for-development  
ANTHROPIC_API_KEY=mock-anthropic-api-key-for-development
GROQ_API_KEY=mock-groq-api-key-for-development
TOGETHER_API_KEY=mock-together-api-key-for-development
MISTRAL_API_KEY=mock-mistral-api-key-for-development
GEMINI_API_TOKEN=mock-gemini-api-token-for-development

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Server Configuration
HOST=0.0.0.0
PORT=11235
TRANSPORT=sse

# Development Settings
PYTHON_ENV=development
DEBUG=true
LOG_LEVEL=info

# Crawl4AI Settings
CACHE_DIR=/home/vscode/.cache/crawl4ai
DATA_DIR=/home/vscode/.local/share/crawl4ai

# Browser Settings
HEADLESS=true
BROWSER_TYPE=chromium

# Performance Settings
MAX_CONCURRENT_CRAWLS=10
REQUEST_TIMEOUT=30
SCREENSHOT_WAIT_FOR=2

# MCP Settings
MCP_ENABLED=true
MCP_WS_PATH=/mcp/ws
MCP_SSE_PATH=/mcp/sse

# Feature Flags
ENABLE_SCREENSHOTS=true
ENABLE_PDF_EXPORT=true
ENABLE_JS_EXECUTION=true
ENABLE_HEALTH_CHECK=true

# JWT Settings (for API authentication)
JWT_SECRET=development-secret-key-not-for-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_DELTA=86400
